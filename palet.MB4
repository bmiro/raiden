2150 *MNTPALE
2160    peca = 1
2170    for pil = 1 to PILES 
2180        for pecapila = 1 to npcspila(pil)
2190            gosub *AGAFPILA
2200            gosub *POSAPALE
2210            peca = peca + 1
2220        next
2230    next
2240    return
    
2260 *DESPALE
2270    for tipus = 1 to TP
2280        munt = ordre(tipus)
2290        while alloc(munt) > 0
2300            gosub *AGAFPALE
2310            gosub *POSADEST
2320        wend
2330    next
2340    return

2350 *AGAFPILA
2360    mov Pila(pil)
2370    Prvsnl = Pila(pil)
2380    Prvsnl.z = Prvsnl.z - ZTPR + (npcspila!(pil) - pecapila) * HDISC
2390    ovrd VLENT
2400    mvs Prvsnl
2410    gosub *TPINCA
