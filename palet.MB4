2430    ovrd VNORMAL
2440    return

2450 *POSAPALE
2460    mov Pale(tipusP(peca))
2470    Prvsnl = Pale(tipusP(peca))
2480    Prvsnl.z = Prvsnl.z - ZTPR + alloc(tipusP(peca)) * HDISC
2490    ovrd VLENT
2500    mvs Prvsnl
2510    gosub *OPINCA
2520    mvs Pale(tipusP(peca))
2530    ovrd VNORMAL
2540    alloc(tipusP(peca)) = alloc(tipusP(peca)) + 1
2550    return

2560 *AGAFPALE
2570    mov PaleOut(munt)
2580    Prvsnl = PaleOut(munt)
2590    Prvsnl.z = Prvsnl.z - ZTPT + (alloc(munt) - 1) * HDISC
2600    ovrd VLENT
2610    mvs Prvsnl
2620    gosub *TPINCA
2630    mvs PaleOut(munt)
2640    ovrd VNORMAL
2650    alloc(munt) = alloc(munt) - 1
2660    return

2670 *POSADEST
2680    mov PalI(munt)
2690    ovrd VLENT
2700    mvs PalF(munt)
2710    gosub *OPINCA
2720    mvs PalI(munt)
2730    ovrd VNORMAL
2740 	return
